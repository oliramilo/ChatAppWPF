<Window x:Class="Client.PrivateMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="PrivateMessageBox" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="PrivateMessages" Margin="71,22,71,142" IsEnabled="false"/>
        <Button x:Name="CloseButton"  Content="Close" HorizontalAlignment="Center" Margin="0,336,0,0" VerticalAlignment="Top" Height="51" Width="208" Click="OnClickClose"/>

    </Grid>
</Window>
